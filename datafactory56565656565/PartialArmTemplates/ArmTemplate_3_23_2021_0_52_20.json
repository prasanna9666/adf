									{
										"source": {
											"type": "String",
											"ordinal": 1
										},
										"sink": {
											"name": "Columnundefined",
											"type": "String",
											"physicalType": "UTF8"
										}
									}
								],
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
